function add_abbr
    abbr --add nv nvim
    abbr --add vim nvim
    abbr --add v nvim
    abbr --add c clear
    abbr --add e eza
    abbr --add ea eza -a
    abbr --add eal eza -al
    abbr --add el eza -l
    abbr --add et eza -T
    abbr --add e1 eza -1
    abbr --add etg eza -T --git-ignore
end
